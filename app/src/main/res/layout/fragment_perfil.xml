<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorDuvider"
    tools:context=".PerfilFragment">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="220dp"
        android:layout_height="204dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="36dp"
        android:src="@drawable/sgdbottomsheet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="530dp"
        android:layout_marginTop="4dp"
        android:background="@color/colorDuvider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nome Completo:"
            app:layout_constraintBottom_toTopOf="@+id/textViewNome"
            app:layout_constraintStart_toStartOf="@+id/textViewNome" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email:"
            app:layout_constraintBottom_toTopOf="@+id/textViewEmail"
            app:layout_constraintStart_toStartOf="@+id/textViewEmail" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Data de Nascimento:"
            app:layout_constraintBottom_toTopOf="@+id/textViewDataNascimento"
            app:layout_constraintStart_toStartOf="@+id/textViewDataNascimento" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Telefone:"
            app:layout_constraintBottom_toTopOf="@+id/textViewtelefone"
            app:layout_constraintStart_toStartOf="@+id/textViewtelefone" />

        <TextView
            android:id="@+id/textViewDataNascimento"
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:layout_marginTop="36dp"
            android:background="@drawable/edit_text_infoperfil"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textViewEmail"
            app:layout_constraintStart_toStartOf="@+id/textViewEmail"
            app:layout_constraintTop_toBottomOf="@+id/textViewEmail" />

        <Button
            android:id="@+id/buttonDeslogar"
            android:layout_width="147dp"
            android:layout_height="49dp"
            android:layout_marginTop="36dp"
            android:text="Deslogar"
            app:layout_constraintEnd_toEndOf="@+id/textViewtelefone"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toStartOf="@+id/textViewtelefone"
            app:layout_constraintTop_toBottomOf="@+id/textViewtelefone" />

        <Button
            android:id="@+id/buttonAtualizar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Atualizar"
            app:layout_constraintEnd_toEndOf="@+id/buttonDeslogar"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/buttonDeslogar"
            app:layout_constraintTop_toBottomOf="@+id/buttonDeslogar" />

        <TextView
            android:id="@+id/textViewNome"
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:layout_marginStart="75dp"
            android:layout_marginTop="76dp"
            android:layout_marginEnd="76dp"
            android:background="@drawable/edit_text_infoperfil"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewtelefone"
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:layout_marginTop="36dp"
            android:background="@drawable/edit_text_infoperfil"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textViewDataNascimento"
            app:layout_constraintStart_toStartOf="@+id/textViewDataNascimento"
            app:layout_constraintTop_toBottomOf="@+id/textViewDataNascimento" />

        <TextView
            android:id="@+id/textViewEmail"
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:layout_marginTop="36dp"
            android:background="@drawable/edit_text_infoperfil"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textViewNome"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textViewNome"
            app:layout_constraintTop_toBottomOf="@+id/textViewNome" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_person_cadastrar"
            app:layout_constraintBottom_toBottomOf="@+id/textViewNome"
            app:layout_constraintEnd_toEndOf="@+id/textViewNome"
            app:layout_constraintTop_toTopOf="@+id/textViewNome" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_email_cadastrar"
            app:layout_constraintBottom_toBottomOf="@+id/textViewEmail"
            app:layout_constraintEnd_toEndOf="@+id/textViewEmail"
            app:layout_constraintTop_toTopOf="@+id/textViewEmail" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_data_cadastrar"
            app:layout_constraintBottom_toBottomOf="@+id/textViewDataNascimento"
            app:layout_constraintEnd_toEndOf="@+id/textViewDataNascimento"
            app:layout_constraintTop_toTopOf="@+id/textViewDataNascimento" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_baseline_local_phone_24"
            app:layout_constraintBottom_toBottomOf="@+id/textViewtelefone"
            app:layout_constraintEnd_toEndOf="@+id/textViewtelefone"
            app:layout_constraintTop_toTopOf="@+id/textViewtelefone" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>